    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.actaCesion ){",
        "\treturn {",
        "\t\t//map actaCesion variable to expected task contract input",
        "\t\tactaCesionInput: {",
        "\t\t\tfirma: $data.actaCesion.firma !== undefined ? $data.actaCesion.firma : null,",
        "\t\t\tfechaNotificacion: $data.actaCesion.fechaNotificacion !== undefined ? $data.actaCesion.fechaNotificacion : null,",
        "\t\t\tfechaFirma: $data.actaCesion.fechaFirma !== undefined ? $data.actaCesion.fechaFirma : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
